   end
   if trailtb[i].life <= 0 then
    del(trailtb, trailtb[i])
   end
  end
  
   
  --print mactor
  if xoomph > 0 then xoomph1() end
  if doomph > 0 then doomph1() end
  if mactor.dcd >0 then
   pal(8,0)
   pal(11,0)
