function checkpoint(x,y)
 if check_anim == 0 then
  strx = x
  stry = y
  check_anim = 6
 end
 
 for i =2,6 do
  add_actor(strx+6*((7-check_anim)/2)*sin(i/8),
  stry+6*((7-check_anim)/2)*cos(i/8),5,2)
 end
 
 if check_anim > 0 then
  check_anim -= 1
 end
end


